        "avh4/elm-color": "1.0.0 <= v < 2.0.0",
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/regex": "1.0.0 <= v < 2.0.0",
        "justinmimbs/date": "4.0.1 <= v < 5.0.0",
        "rtfeldman/elm-hex": "1.0.0 <= v < 2.0.0",
        "elm/json": "1.1.3 <= v < 2.0.0",
        "elm/file": "1.0.5 <= v < 2.0.0"
    },
    "test-dependencies": {}
