{
    "type": "package",
    "name":  "yoghurt-x86/easy-forms",
    "summary": "Simple and fast way to build forms",
    "license": "BSD-3-Clause",
    "version": "1.0.0",
    "source-directories": [
        "src"
    ],
    "exposed-modules": [
        "Form.Html",
        "Form.Field",
        "Form.SimpleFields",
        "Form.Internals"
